    for s in s.split(","):
        s = s.lower()
        s = s.strip()
        s = re.sub(r" +", " ", s)
        s = re.sub(r" *\&gt; *", " &gt;", s)
        s = re.sub(r" *\&lt; *", " &lt;", s)
        s = re.sub(r" *\+ *", " +", s)
        s = re.sub(r"\[.*?\]",
            lambda m: _encode_space(m.group(0)), s)
        s = re.sub(r":contains\(.*?\)",
            lambda m: _encode_space(m.group(0)), s)
        self.append([])
        for s in s.split(" "):
            if not s.startswith(("&gt;", "&lt;", "+")):
                self[-1].append((" ", Selector(s)))
            elif s.startswith("&gt;"):
                self[-1].append(("&gt;", Selector(s[1:])))
            elif s.startswith("&lt;"):

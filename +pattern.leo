        j = s.find("&lt;/%s&gt;" % t, i + 1)
        opened, closed = s[i:j].count("&lt;%s" % t), 1
        while opened &gt; closed and j &gt;= 0:
            k = s.find("&lt;/%s&gt;" % t, j + 1)
            opened += s[j:k].count("&lt;%s" % t)
            closed += 1
            j = k
        if i &lt; 0:
            return string
        if j &lt; 0:
            return string[:i]
        string = string[:i] + string[j + len(t) + 3:]; s = string.lower()
    return string



<t tx="karstenw.20230303123148.88">def _repulse(self, node1, node2):
    # Updates Node.force with the repulsive force.
    dx, dy, d, d2 = self._distance(node1, node2)
    if d &lt; self.repulsion:
        f = self.k ** 2 / d2
        node2.force.x += f * dx
        node2.force.y += f * dy
        node1.force.x -= f * dx
        node1.force.y -= f * dy


cp._cperror._HTTPErrorTemplate = \
    "&lt;h1&gt;%(status)s&lt;/h1\n&gt;&lt;p&gt;%(message)s&lt;/p&gt;\n&lt;pre&gt;%(traceback)s&lt;/pre&gt;"


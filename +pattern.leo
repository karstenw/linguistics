<t tx="karstenw.20230303142634.4">def _basename(folder):
    # [Gmail]/INBOX =&gt; inbox
    f = folder.replace("[Gmail]/", "")
    f = f.replace("[Gmail]", "")
    f = f.replace("Mail", "")   # "Sent Mail" alias = "sent".
    f = f.replace("INBOX.", "") # "inbox.sent" alias = "sent".
    f = f.lower()
    f = f.strip()
    return f



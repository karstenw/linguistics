<t tx="karstenw.20230303134440.12">def find_lexeme(self, verb):
    """ For a regular verb (base form), returns the forms using a rule-based approach.
    """
    v = verb.lower()
    if v.endswith(("arse", "erse", "irse")):
        # Reflexive verbs: calmarse (calmar) =&gt; me calmo.
        b = v[:-4]
    else:
        b = v[:-2]
    if v.endswith("ar") or not v.endswith(("er", "ir")):
        # Regular inflection for verbs ending in -ar.
        return [v,
            b + "o", b + "as", b + "a", b + "amos", b + "is", b + "an", b + "ando",
            b + "", b + "aste", b + "", b + "amos", b + "asteis", b + "aron", b + "ado",
            b + "aba", b + "abas", b + "aba", b + "bamos", b + "abais", b + "aban",
            v + "", v + "s", v + "", v + "emos", v + "is", v + "n",
            v + "a", v + "as", v + "a", v + "amos", v + "ais", v + "an",
            b + "a", v[:-1] + "d",
            b + "e", b + "es", b + "e", b + "emos", b + "is", b + "en",
            v + "a", v + "as", v + "a", b + "ramos", v + "ais", v + "an"]
    else:
        # Regular inflection for verbs ending in -er and -ir.
        p1, p2 = v.endswith("er") and ("e", "") or ("i", "e")
        return [v,
            b + "o", b + "es", b + "e", b + p1 + "mos", b + p2 + "is", b + "en", b + "iendo",
            b + "", b + "iste", b + "i", b + "imos", b + "isteis", b + "ieron", b + "ido",

            b0 + "s", b0 + "s", b0 + "", b1 + "ons", b1 + "ez", b1 + "ent", b1 + "ant", b + "u",
            b + "is", b + "is", b + "it", b1 + "mes", b1 + "tes", b1 + "irent",
